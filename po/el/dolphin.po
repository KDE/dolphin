#: dolphinmainwindow.cpp:1940
#, kde-format
msgctxt "@info:tooltip"
msgid "Search for files and folders"
msgstr "Αναζήτηση για αρχεία και φακέλους"

#: dolphinmainwindow.cpp:1942
#, fuzzy, kde-kuit-format
#| msgctxt "@info:whatsthis find"
#| msgid ""
