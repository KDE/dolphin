#: dolphinmainwindow.cpp:1975
#, kde-format
msgctxt "@action:inmenu"
msgid "Show Target"
msgstr "Sýna úttak"

#: dolphinmainwindow.cpp:1981
#, kde-format
msgctxt "@action:inmenu"
msgid "Open in New Tab"
msgstr "Opna í nýjum flipa"

#: dolphinmainwindow.cpp:1986
#, kde-format
msgctxt "@action:inmenu"
msgid "Open in New Tabs"
