msgid "Next Tab"
msgstr "اللسان التالي"

#: dolphinmainwindow.cpp:1962
#, kde-format
msgctxt "@action:inmenu"
msgid "Activate Next Tab"
msgstr "فعّل اللسان التالي"

#: dolphinmainwindow.cpp:1968
#, kde-format
